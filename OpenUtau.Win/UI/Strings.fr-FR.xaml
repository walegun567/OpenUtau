    <system:String x:Key="prefs.appearance.theme.dark">Nuit</system:String>
    <system:String x:Key="prefs.appearance.theme.note">Note: relancez OpenUTAU pour appliquer le thème.</system:String>

    <!-- Dialogs -->
    <system:String x:Key="dialogs.noresampler.caption">Aucun resampler</system:String>
    <system:String x:Key="dialogs.noresampler.message">Aucun resampler! Ajoutez le resampler voulu dans le dossier "Resampler" de OpenUTAU, puit choisissez-le dans Préférances!</system:String>
    <system:String x:Key="dialogs.about.caption">About OpenUTAU</system:String>
    <system:String x:Key="dialogs.about.message">
        OpenUtau est logiciel de synthétiseur vocal open source pour la communauté UTAU, avec une interface moderne et un support phonétique intelligent. Allez sur notre Github.
    </system:String>
    <system:String x:Key="dialogs.export.caption">Exporter</system:String>
    <system:String x:Key="dialogs.export.savefirst">Sauvegardez le fichier d'abord</system:String>

    <!-- Tracks -->
    <system:String x:Key="tracks.remove">Retirer</system:String>
    <system:String x:Key="tracks.moveup">Monter</system:String>
    <system:String x:Key="tracks.movedown">Descendre</system:String>

    <!-- Errors -->
    <system:String x:Key="errors.caption">Erreur</system:String>
    <system:String x:Key="warning">Attention</system:String>
    <system:String x:Key="warning.asciipath">Le dossier d'installation d'OpenUTAU "{0}" contient des lettres non-ASCII, les resamplers .exe peuvent ne pas marcher.</system:String>

    <!-- Tips -->
    <system:String x:Key="tip.lyricbox.prev">Shift+Tab: Note Précédente</system:String>
    <system:String x:Key="tip.lyricbox.next">Tab: Note Suivante</system:String>
