    <system:String x:Key="prefs.caption">Preferencias</system:String>
    <system:String x:Key="prefs.general">General</system:String>
    <system:String x:Key="prefs.general.paths">Rutas</system:String>
    <system:String x:Key="prefs.general.paths.singer">Rutas del cantante</system:String>
    <system:String x:Key="prefs.general.paths.add">Añadir</system:String>
    <system:String x:Key="prefs.general.paths.remove">Quitar</system:String>
    <system:String x:Key="prefs.audio">Audio</system:String>
    <system:String x:Key="prefs.audio.playback">Reproducir</system:String>
    <system:String x:Key="prefs.audio.rendering">Renderizado</system:String>
    <system:String x:Key="prefs.audio.rendering.resampler.preview">Remuestreo de vista previa</system:String>
    <system:String x:Key="prefs.audio.rendering.resampler.export">Remuestreo de exportación</system:String>
    <system:String x:Key="prefs.audio.rendering.resampler.note">
        Nota: para usar un remuestreador externo, añada el archivo DLL o EXE del remuestreador en la carpeta Resamplers en la ruta de instalación de OpenUtau.
    </system:String>
    <system:String x:Key="prefs.appearance">Apariencia</system:String>

    <!-- Dialogs -->
    <system:String x:Key="dialogs.noresampler.caption">No hay un remuestreador</system:String>
    <system:String x:Key="dialogs.noresampler.message">¡No hay un remuestreador! Añada su remuestreador favorito exe o dll en la carpeta Resamplers.</system:String>
    <system:String x:Key="dialogs.about.caption">Acerca de OpenUTAU</system:String>
    <system:String x:Key="dialogs.about.message">
        El objetivo de OpenUtau es ser un entorno de edición de código abierto para la comunidad de UTAU, ofreciendo una experiencia de usuario moderna y soporte fonológico inteligente. Visítanos en Github.
    </system:String>

    <!-- Errors -->
    <system:String x:Key="errors.caption">Error</system:String>

</ResourceDictionary>